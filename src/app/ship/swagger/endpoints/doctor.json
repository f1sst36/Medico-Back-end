            {
                "name": "page",
                "in": "query",
                "description": "Number of page",
                "required": true,
                "schema": {
                    "type": "number",
                    "example": "page=1"
                }
            },
            {
                "name": "count",
                "in": "query",
                "description": "Count of items per page",
                "required": true,
                "schema": {
                    "type": "number",
                    "example": "count=3"
                }
            },
            {
                "name": "fio",
                "in": "query",
                "description": "Doctor's FIO",
                "required": false,
                "schema": {
                    "type": "string",
                    "example": "fio=Ivan Ivanov"
